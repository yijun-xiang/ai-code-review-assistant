{
  "css.validate": false,
  "scss.validate": false,
  "tailwindCSS.emmetCompletions": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "eslint.workingDirectories": [
    "./frontend",
    "./backend"
  ],
  "docker.languageserver.diagnostics.enable": false,
  "docker.languageserver.diagnostics.deprecatedMaintainer": "ignore",
  "docker.languageserver.diagnostics.instructionCasing": "ignore",
  "docker.languageserver.diagnostics.instructionCmdMultiple": "ignore"
}